<odoo>
    <record id="bs_sale_order_seq" model="ir.sequence">
        <field name="name">BS Sale Order</field>
        <field name="code">bs.sale.order</field>
        <field name="prefix">S</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>


    <record id="bs_purchase_order_seq" model="ir.sequence">
        <field name="name">BS Purchase Order</field>
        <field name="code">bs.purchase.order</field>
        <field name="prefix">P</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>